{
  "manifest_version": 3,
  "name": "YouTube BackInTime",
  "version": "0.0.3",
  "description": "This extension adds a Hotkey "B" to YouTube for back when random rewind a video. \n Python brrrrr",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ]
}

